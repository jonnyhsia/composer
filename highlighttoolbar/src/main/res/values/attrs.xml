<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="HighlightToolbar">
        <attr name="highlightTitle" format="string|reference"/>
        <attr name="highlightSubTitle" format="string|reference"/>
        <attr name="icon" format="reference"/>
        <attr name="dividerVisibility" format="enum">
            <enum name="SHOW" value="0"/>
            <enum name="HIDE" value="8"/>
        </attr>
    </declare-styleable>

</resources>